{
  "executable": "SentToDo",
  "splashscreen": {
    "imageFile": ""
  },
  "name": "SentToDo",
  "author": "",
  "icon": "bin/Assets/icon_256x256ico.ico",
  "singleInstance": true,
  "environment": "Production",
  "build": {
    "appId": "com.SentToDo.app",
    "productName": "SentToDo",
    "copyright": "Copyright Â© 2022",
    "buildVersion": "1.0.0",
    "compression": "maximum",
    "win": {
      "icon": "bin/Assets/icon_256x256ico.ico"
    },
    "directories": {
      "output": "../../../bin/Desktop"
    },
    "extraResources": [
      {
        "from": "./bin",
        "to": "bin",
        "filter": [ "**/*" ]
      }
    ],
    "files": [
      {
        "from": "./ElectronHostHook/node_modules",
        "to": "ElectronHostHook/node_modules",
        "filter": [ "**/*" ]
      },
      "**/*"
    ]
  }
}